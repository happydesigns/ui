{
  "name": "@happydesigns/ui-base",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@10.14.0",
  "author": "happydesigns",
  "homepage": "https://github.com/happydesigns",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev",
    "dev:play": "nuxi dev .playground",
    "prepare": "nuxt prepare && nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "changelogen --release",
    "release:pre": "changelogen --release --prerelease",
    "release:alpha": "changelogen --release --versionSuffix alpha.0",
    "release:beta": "changelogen --release --versionSuffix beta.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.61",
    "@iconify-json/simple-icons": "^1.2.46",
    "@nuxt/content": "^3.6.3",
    "@nuxt/ui-pro": "^3.3.0",
    "nuxt": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@antfu/ni": "^25.0.0",
    "@nuxt/eslint": "^1.8.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ]
  },
  "resolutions": {
    "@happydesigns/ui-base": "workspace:*"
  }
}
