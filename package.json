{
  "name": "@happydesigns/ui-base",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@10.17.0",
  "author": "happydesigns",
  "homepage": "https://github.com/happydesigns",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxt dev",
    "play": "nuxt dev .playground",
    "docs": "nuxt dev docs --extends docus",
    "prepare": "nuxt prepare && nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "changelogen --release",
    "release:pre": "changelogen --release --prerelease",
    "release:alpha": "changelogen --release --versionSuffix alpha.0",
    "release:beta": "changelogen --release --versionSuffix beta.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/simple-icons": "^1.2.52",
    "@nuxt/content": "^3.7.1",
    "@nuxt/ui": "4.0.0-alpha.2",
    "nuxt": "^4.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@antfu/ni": "^26.0.1",
    "@nuxt/eslint": "^1.9.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ]
  },
  "resolutions": {
    "@happydesigns/ui-base": "workspace:*"
  }
}
