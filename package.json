{
  "name": "@happydesigns/ui-base",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@10.10.0",
  "author": "happydesigns",
  "homepage": "https://github.com/happydesigns",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev",
    "dev:play": "nuxi dev .playground",
    "prepare": "nuxt prepare && nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "changelogen --release",
    "release:pre": "changelogen --release --prerelease",
    "release:alpha": "changelogen --release --versionSuffix alpha.0",
    "release:beta": "changelogen --release --versionSuffix beta.0"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.39",
    "@iconify-json/simple-icons": "^1.2.33",
    "@nuxt/content": "3.4.0",
    "@nuxt/ui-pro": "^3.1.0",
    "nuxt": "^3.17.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@antfu/ni": "^24.3.0",
    "@nuxt/eslint": "^1.3.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "vue-demi",
      "workerd"
    ]
  },
  "resolutions": {
    "@happydesigns/ui-base": "workspace:*"
  }
}
